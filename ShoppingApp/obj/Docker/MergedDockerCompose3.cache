services:
  shopping.api:
    build:
      context: C:\Dev\MicroServices\ShoppingApp
      dockerfile: Shopping.API/Dockerfile
    environment:
      ASPNETCORE_ENVIRONMENT: Development
    image: shoppingapi
    ports:
    - target: 80
  shopping.client:
    build:
      context: C:\Dev\MicroServices\ShoppingApp
      dockerfile: Shopping.Client/Dockerfile
    environment:
      ASPNETCORE_ENVIRONMENT: Development
    image: shoppingclient
    ports:
    - target: 80
    volumes:
    - C:\Users\ombirade\AppData\Roaming\Microsoft\UserSecrets:/root/.microsoft/usersecrets:ro
version: '3.4'